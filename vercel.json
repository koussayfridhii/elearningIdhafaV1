{
    "version": 2,
    "cleanUrls": true,
    "builds": [
      {
        "src": "elearningIdhafaV1/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles_build"
        }
      }
    ],
    "build": "[Django] build --output public",
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "elearningIdhafaV1/wsgi.py"
      },
    ]
    
  }